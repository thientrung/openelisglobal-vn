	<queryString>
		<![CDATA[SELECT EXTRACT(MONTH FROM s.received_date) as month,
	EXTRACT(DOW FROM s.received_date) + 1 as day,
	s.accession_number,
	prj.id AS project_id,prj.name AS project_name,
	p.external_id,tos.description as type_of_sample,
	per.first_name as patient_name,
	p.birth_date,age.value AS age, age_unit.value AS age_unit,
    CASE
            WHEN p.gender::text = 'M'::text THEN '1'::text
            WHEN p.gender::text = 'F'::text THEN '0'::text
            ELSE 'Không xác định'::text
        END AS gender,
	per.street_address, ward.value as ward, district_value.dict_entry as district,
	(((CASE
            WHEN per.street_address IS NOT NULL OR rtrim(per.street_address::text, ' '::text) <> ''::text THEN per.street_address
            ELSE ''::character varying
        END::text || ', '::text) || ward.value::text) || ', '::text) || district_value.dict_entry::text AS address,

    city_value.dict_entry AS city,
	p.chart_number,
	org.id AS organization_id,
    org.name AS organization_name,
    diagnosis.value AS diagnosis,
	s.illness_date,
	si.collection_date,
    s.received_date,
    a.released_date AS result_date,
    a.completed_date,
	CASE WHEN a.completed_date is null THEN '' ELSE to_char(a.completed_date, 'DD-MM-YY') END as do_date,
	CASE
            WHEN s.illness_date IS NOT NULL AND si.collection_date IS NOT NULL THEN (date_part('day'::text, si.collection_date - s.illness_date::timestamp with time zone) + 1::double precision)::text
            ELSE 'N/A'::text
        END AS number_of_day,
        CASE
            WHEN s.received_date IS NOT NULL AND a.released_date IS NOT NULL THEN (date_part('day'::text, a.released_date - s.received_date::timestamp with time zone) + 1::double precision)::text
            ELSE 'N/A'::text
        END AS execution_days,
    ps.first_name AS doctor,
    pt.description AS patient_type,
    payment.value AS payment_type,
    t.id AS test_id,
    t.name AS test_name,
    t.description AS test_description,
    ts.id AS test_section_id,
    ts.description AS test_section,
        CASE
            WHEN r.result_type::text = 'D'::text THEN dict.dict_entry
            ELSE r.value
        END AS result,
    r.test_result_id,
    r.parent_id,
    r.result_type,

    r.value AS original_result
   FROM sample s
     LEFT JOIN sample_requester sr ON sr.sample_id = s.id AND sr.requester_type_id = 1::numeric
     LEFT JOIN organization org ON sr.requester_id = org.id
     LEFT JOIN sample_item si ON si.samp_id = s.id
     LEFT JOIN type_of_sample tos ON si.typeosamp_id = tos.id
     LEFT JOIN sample_human sh ON sh.samp_id = s.id
     LEFT JOIN patient p ON sh.patient_id = p.id
     LEFT JOIN person per ON p.person_id = per.id
     LEFT JOIN person_address ward ON ward.person_id = per.id AND ward.address_part_id = 8::numeric
     LEFT JOIN person_address district ON district.person_id = per.id AND district.address_part_id = 7::numeric
     LEFT JOIN dictionary district_value ON district.value::text = district_value.id::text
     LEFT JOIN person_address city ON city.person_id = per.id AND city.address_part_id = 3::numeric
     LEFT JOIN dictionary city_value ON city.value::text = city_value.id::text
     LEFT JOIN provider pr ON sh.provider_id = pr.id
     LEFT JOIN person ps ON pr.person_id = ps.id
     LEFT JOIN analysis a ON a.sampitem_id = si.id
     LEFT JOIN test t ON a.test_id = t.id
     LEFT JOIN test_section ts ON t.test_section_id = ts.id
     LEFT JOIN result r ON r.analysis_id = a.id
     LEFT JOIN dictionary dict ON r.value::text = dict.id::text
     LEFT JOIN observation_history payment ON payment.sample_id = s.id AND payment.observation_history_type_id = 2::numeric
     LEFT JOIN observation_history diagnosis ON diagnosis.sample_id = s.id AND diagnosis.observation_history_type_id = 6::numeric
     LEFT JOIN observation_history age ON age.sample_id = s.id AND age.observation_history_type_id = 10::numeric
     LEFT JOIN observation_history age_unit ON age_unit.sample_id = s.id AND age_unit.observation_history_type_id = 11::numeric
     LEFT JOIN patient_patient_type ppt ON ppt.patient_id = p.id
     LEFT JOIN patient_type pt ON ppt.patient_type_id = pt.id
     LEFT JOIN sample_projects sp ON s.id = sp.samp_id
     LEFT JOIN project prj ON sp.proj_id = prj.id
WHERE CASE WHEN $P{startAccessionNumber} is NULL OR $P{startAccessionNumber} = '' THEN 1 = 1
	WHEN $P{endAccessionNumber} IS NULL OR $P{endAccessionNumber} = '' THEN s.accession_number = $P{startAccessionNumber}
	ELSE s.accession_number between $P{startAccessionNumber} AND $P{endAccessionNumber} END
	AND (CASE WHEN ($P{startReceivedDate} IS NULL AND $P{endReceivedDate} IS NULL) OR ($P{startReceivedDate} = '' AND $P{endReceivedDate} = '') THEN 1 = 1
		ELSE (a.completed_date::date BETWEEN to_date($P{startReceivedDate},'dd/MM/yyyy') AND to_date($P{endReceivedDate},'dd/MM/yyyy') AND t.id is not null)  END)
	and a.status_id != 15
  ORDER BY s.accession_number, si.id, t.id]]>
