	</parameter>
	<parameter name="End_Received_Date" class="java.util.Date">
		<defaultValueExpression><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
  s.accession_number,
  CASE WHEN pa.gender='M' THEN 'Nam' WHEN pa.gender='F' THEN 'Nữ' ELSE 'Không xác định' END AS gender,
  CASE WHEN pa.gender = 'M' THEN 1 ELSE 0 END as ismale,
  CASE WHEN result_value.id != 2097 THEN 1 ELSE 0 END as ispositive,
  CASE WHEN ptype.id = 7 THEN 1
    WHEN ptype.id = 8 THEN 2 ELSE 3 END as patient_type,
  CASE WHEN payment_value.dict_entry ilike '%bhyt%' THEN 1 ELSE 0 END as bhyt,
  CASE WHEN department_value.dict_entry in ('Nội I','Nội II','Nội III','Nội IV','Nội V','Nội VI','Ngoại','Cấp Cứu')
    THEN 1 ELSE 0 END as noi

FROM sample s
  INNER JOIN sample_item si ON si.samp_id = s.id
  INNER JOIN analysis a ON a.sampitem_id = si.id
  INNER JOIN test t ON a.test_id = t.id
  INNER JOIN sample_human sh ON sh.samp_id = s.id
  INNER JOIN patient pa ON sh.patient_id = pa.id
  -- Result
  INNER JOIN result r ON r.analysis_id = a.id
  LEFT JOIN dictionary result_value ON r.value::text = result_value.id::text
  -- Patient Type
  LEFT JOIN patient_patient_type ppt ON pa.id = ppt.patient_id
  LEFT JOIN patient_type ptype ON ppt.patient_type_id = ptype.id
  -- Department
  LEFT JOIN observation_history department ON s.id = department.sample_id AND department.observation_history_type_id = 9::numeric
  LEFT JOIN dictionary department_value ON department.value::text = department_value.id::text
  -- Payment
  LEFT JOIN observation_history payment ON s.id = payment.sample_id AND payment.observation_history_type_id = 2::numeric
  LEFT JOIN dictionary payment_value ON payment.value::text = payment_value.id::text

WHERE s.received_date BETWEEN $P{Start_Received_Date} AND $P{End_Received_Date}
  AND t.id = 365
ORDER BY gender,accession_number]]>
	</queryString>
